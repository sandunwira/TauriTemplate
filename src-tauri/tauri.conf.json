{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "TauriTemplate",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "close": true,
        "hide": true,
        "show": true,
        "maximize": true,
        "minimize": true,
        "unmaximize": true,
        "unminimize": true
      },
      "fs": {
        "all": true
      },
      "notification": {
        "all": true
      },
      "dialog": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "publisher": "Publisher Name",
      "category": "Utility",
      "copyright": "Publisher Name",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "TauriTemplate",
      "shortDescription": "TauriTemplate",
      "longDescription": "TauriTemplate is a template created for Tauri",
      "resources": [],
      "externalBin": [],
      "targets": "all",
      "deb": {
        "depends": []
      },
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "appimage": {
        "bundleMediaFramework": true
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "allowDowngrades": false,
        "wix": {
          "language": "en-US",
          "bannerPath": "../src/assets/images/installer/banner.png",
          "dialogImagePath": "../src/assets/images/installer/dialogImage.png"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://raw.githubusercontent.com/sandunwira/TauriUpdater/main/tauriTemplate.json"
      ],
      "windows": {
        "installMode": "passive"
      },
      "pubkey": ""
    },
    "windows": [
      {
        "title": "TauriTemplate",
        "minWidth": 1000,
        "minHeight": 600,
        "url": "index.html",
        "fullscreen": false,
        "focus": true,
        "resizable": true,
        "decorations": false,
        "center": true
      }
    ]
  }
}
